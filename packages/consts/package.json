{
  "name": "@nullvoxpopuli/limber-consts",
  "version": "0.0.0",
  "description": "repo constants for helping out node scripts",
  "main": "index.js",
  "author": "NullVoxPopuli",
  "type": "commonjs",
  "license": "MIT",
  "private": true,
  "scripts": {
    "lint:prettier:fix": "prettier --no-error-on-unmatched-pattern --cache --cache-strategy content -w **/*.{js,ts,gjs,gts,hbs,css}",
