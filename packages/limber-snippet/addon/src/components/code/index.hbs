{{#if this.isPreview}}

  <pre><code></code></pre>

{{else}}

    <iframe
      {{this.messaging.postMessage this.code.value}}
      {{this.messaging.onMessage}}
      title={{this.title}}
      src="{{this.host}}"
      style={{this.htmlSafe (concat "height: calc(1.5rem * " this.lines ");")}}
      ...attributes
    >
    </iframe>

{{/if}}