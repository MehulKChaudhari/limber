{
  "name": "@nullvoxpopuli/limber-codemirror",
  "version": "0.0.0",
  "description": "custom build of codemirror for use in Limber",
  "main": "index.js",
  "author": "NullVoxPopuli",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "node build.js",
    "check:types": "tsc --build",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel 'lint:!(fix)'",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix"
  },
  "dependencies": {
    "@codemirror/autocomplete": "0.18.6",
    "@codemirror/basic-setup": "0.18.2",
    "@codemirror/closebrackets": "0.18.0",
    "@codemirror/commands": "0.18.3",
    "@codemirror/comment": "0.18.1",
    "@codemirror/highlight": "0.18.4",
    "@codemirror/history": "0.18.1",
    "@codemirror/lang-javascript": "0.18.0",
    "@codemirror/lang-markdown": "0.18.3",
    "@codemirror/search": "0.18.4",
    "@codemirror/state": "0.18.7",
    "@codemirror/text": "0.18.0",
    "@codemirror/view": "0.18.16",
    "@nullvoxpopuli/horizon-theme": "*",
    "broccoli-funnel": "^3.0.8"
  },
  "devDependencies": {
    "@nullvoxpopuli/eslint-configs": "1.4.0",
    "esbuild": "0.12.24",
    "esbuild-plugin-alias": "^0.1.2",
    "recursive-copy": "^2.0.13"
  },
  "engines": {
    "node": "14.17.0"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
