{
  "name": "limber-monorepo",
  "private": true,
  "author": "NullVoxPopuli",
  "scripts": {
    "dev": "node ./dev/index.js",
    "prepare": "pnpm build",
    "release": "changeset publish",
    "build": "pnpm turbo build --filter=limber^...",
    "lint:fix": "pnpm --filter='*' lint:fix",
    "lint": "pnpm turbo lint",
    "start": "pnpm turbo start --filter=limber"
  },
  "engines": {
    "node": ">= 16"
  },
  "volta": {
    "node": "18.15.0",
    "yarn": "1.22.19",
    "npm": "9.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@changesets/changelog-github": "^0.4.8",
    "@changesets/cli": "^2.26.0",
    "@nullvoxpopuli/eslint-configs": "^3.1.3",
    "concurrently": "^7.6.0",
    "eslint": "^8.35.0",
    "prettier": "^2.8.4",
    "turbo": "^1.8.3"
  },
  "packageManager": "pnpm@7.16.1",
  "pnpm": {
    "overrides": {
      "@glimmer/component": "^1.1.2",
      "@glimmer/tracking": "^1.1.2",
      "ember-async-data": "^0.7.0",
      "ember-auto-import": "^2.6.0",
      "@types/eslint": "^8.0.0"
    },
    "allowedDeprecatedVersions": {
      "source-map-url": "^0.3.0",
      "core-js": "^2.0.0"
    },
    "peerDependencyRules": {
      "allowAny": [
        "ember-modifier"
      ],
      "ignoreMissing": [
        "webpack"
      ]
    },
    "notes": {},
    "patchedDependencies": {
      "ember-on-resize-modifier@1.1.0": "patches/ember-on-resize-modifier@1.1.0.patch",
      "ember-async-data@0.7.0": "patches/ember-async-data@0.7.0.patch"
    }
  }
}
